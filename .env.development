# Development Environment Variables for AgentLab
# This file contains development-specific configuration

# Database Configuration
POSTGRES_DB=agentlab_dev
POSTGRES_USER=agentlab_user
POSTGRES_PASSWORD=dev_password
POSTGRES_PORT=5432

# Redis Configuration
REDIS_PASSWORD=dev_redis_password
REDIS_PORT=6379

# API Configuration
API_PORT=8000
ENV=development
DEBUG=true
LOG_LEVEL=debug

# Web Application Configuration
WEB_PORT=3000
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=dev_nextauth_secret_change_in_production

# External API Keys (set these in your local .env file)
CLAUDE_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
OLLAMA_BASE_URL=http://localhost:11434

# Development Tools
PGADMIN_EMAIL=admin@agentlab.local
PGADMIN_PASSWORD=dev_admin_password
REDIS_COMMANDER_USER=admin
REDIS_COMMANDER_PASSWORD=dev_admin_password

# Nginx Configuration
HTTP_PORT=80
HTTPS_PORT=443

# Hot Reload Settings
CHOKIDAR_USEPOLLING=true
WATCHPACK_POLLING=true

# Development Debugging
PYTHONPATH=/app
RELOAD=true